"\t\t\t\t\t\t<br />\n"
"\t\t\t\t\t\t<br />[wplp_campaign_link campaign=\"\" landingpage=\"\" imglink="
"\"\" textlink=\"\"] Custom Use\n"
"\t\t\t\t\t\t<br />\n"
